{
  "name": "repeka",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-4": {
      "": "src/"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fracz/doctrine-json-odm"
    }
  ],
  "require": {
    "php": ">=7.1",
    "beberlei/assert": "~2.0",
    "cocur/slugify": "^2.4",
    "doctrine/dbal": "2.6.x-dev",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-migrations-bundle": "^1.2",
    "doctrine/orm": "2.6.x-dev",
    "dunglas/doctrine-json-odm": "dev-null-fix",
    "ext-mbstring": "*",
    "ext-pdo_pgsql": "*",
    "ext-soap": "*",
    "kherge/json": "2.0",
    "m6web/statsd-bundle": "^2.14",
    "myclabs/php-enum": "^1.5",
    "qandidate/symfony-json-request-transformer": "^1.2.2",
    "respect/validation": "^1.1",
    "ruflin/elastica": "^3.2",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "^3.0.2",
    "symfony/monolog-bundle": "^2.8",
    "symfony/serializer": "^3.2",
    "symfony/swiftmailer-bundle": "^2.3",
    "symfony/symfony": "^3.2",
    "twig/twig": "^1.30"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "^5.3",
    "squizlabs/php_codesniffer": "2.*",
    "symfony/phpunit-bridge": "^3.0"
  },
  "scripts": {
    "symfony-scripts": [
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
    ],
    "post-install-cmd": [
      "@symfony-scripts"
    ],
    "post-update-cmd": [
      "@symfony-scripts"
    ]
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "symlink"
  }
}
