{
  "name": "repeka",
  "license": "proprietary",
  "type": "project",
  "autoload": {
    "psr-4": {
      "Repeka\\": "src/Repeka"
    },
    "classmap": [
      "app/AppKernel.php",
      "app/AppCache.php"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/gronostajo/DoctrineJsonFunctions"
    }
  ],
  "require": {
    "php": ">=7.1",
    "beberlei/assert": "^2.7",
    "cocur/slugify": "^2.5",
    "doctrine/dbal": "^2.6.1",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/doctrine-migrations-bundle": "^1.2",
    "doctrine/orm": "2.6.x-dev",
    "dunglas/doctrine-json-odm": "^0.1",
    "ext-mbstring": "*",
    "ext-pdo_pgsql": "*",
    "ext-soap": "*",
    "m6web/statsd-bundle": "^2.14",
    "myclabs/php-enum": "^1.5",
    "qandidate/symfony-json-request-transformer": "^1.2.2",
    "respect/validation": "dev-master#b83f55e3e97d7103b6eb2eea69e7c4bf66750a0b",
    "ruflin/elastica": "^3.2",
    "sensio/distribution-bundle": "^5.0",
    "sensio/framework-extra-bundle": "3.0.22",
    "symfony/monolog-bundle": "^3.1",
    "symfony/process": "^3.3",
    "symfony/serializer": "^3.3",
    "symfony/symfony": "^3.3",
    "syslogic/doctrine-json-functions": "dev-better-postgres-support"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^2.3",
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "^5.3",
    "squizlabs/php_codesniffer": "2.*",
    "symfony/phpunit-bridge": "^3.0"
  },
  "scripts": {
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-web-dir": "web",
    "symfony-tests-dir": "tests"
  }
}
