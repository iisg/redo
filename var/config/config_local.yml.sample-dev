parameters:
  secret: soo_secret
doctrine:
  dbal:
    host: 127.0.0.1
    user: postgres
    password: postgres
repeka:
  application_url: http://repeka.local
  resource_classes:
    - name: books
      icon: publish-4
      admins:
        - Username: admin
      operators:
        - Username: budynek
        - Username: tester
        - Username: skaner
    - name: dictionaries
      icon: dictionary
      admins:
        - Username: admin
    - name: users
      icon: user
      admins:
        - Username: admin
    - name: cms
      icon: html
      admins:
        - Username: admin
  elasticsearch:
    host: localhost
  xml_import:
    koha: 'http://koha.biblos.pk.edu.pl/cgi-bin/koha/opac-export-simple.pl?op=export&format=marcxml&skip_entity_encoding=1&barcode='
  templating:
    templates_resource_class: cms
    templates:
      homepage: redo/home.twig
      login_form: redo/login-form.twig
  expose_endpoints:
    /about:
      resourceId: 18
      metadata: wyrenderowana_tresc_strony
    /resources/{resourceId}:
      metadata: tresc_strony
m6_statsd:
  servers:
    default:
      address: 'udp://localhost'
